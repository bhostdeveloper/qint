# Maintainer stuff

dist-rpm:
	cp qmailadmin-1.2.13.tar.gz $$HOME/src/rpm/SOURCES
	cp qmailadmin.spec $$HOME/src/rpm/SPECS
	cd $$HOME/src/rpm/SPECS && rpm -ba --sign qmailadmin.spec
	rm -rf htmldist
	mkdir htmldist
	ln NEWS.html htmldist/index.html
	ln $$HOME/src/rpm/SRPMS/qmailadmin-1.2.13-1.src.rpm qmailadmin-1.2.13.tar.gz htmldist

dist-hook:

# use the [^C]* pattern to skip the CVS directory
install-data-local:
	$(mkinstalldirs)         $(DESTDIR)/usr/local/share/qmailadmin
	rm -rf                   $(DESTDIR)/usr/local/share/qmailadmin/html
	$(mkinstalldirs)         $(DESTDIR)/usr/local/share/qmailadmin/html 
	cp $(srcdir)/html/*.html $(DESTDIR)/usr/local/share/qmailadmin/html
	cp $(srcdir)/html/colortable $(DESTDIR)/usr/local/share/qmailadmin/html
	chmod 755                $(DESTDIR)/usr/local/share/qmailadmin/html
	chmod 644                $(DESTDIR)/usr/local/share/qmailadmin/html/*
	rm -rf                   $(DESTDIR)/usr/local/share/qmailadmin/lang
	$(mkinstalldirs)         $(DESTDIR)/usr/local/share/qmailadmin/lang 
	cp $(srcdir)/lang/??     $(DESTDIR)/usr/local/share/qmailadmin/lang
	cp $(srcdir)/lang/??-??  $(DESTDIR)/usr/local/share/qmailadmin/lang
	chmod 755                $(DESTDIR)/usr/local/share/qmailadmin/lang
	chmod 644                $(DESTDIR)/usr/local/share/qmailadmin/lang/*
	$(mkinstalldirs)         $(DESTDIR)/var/www/qmailadmin/images
	cp $(srcdir)/images/*.png $(DESTDIR)/var/www/qmailadmin/images
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
